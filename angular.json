{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "testang2": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "public/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "node_modules/font-awesome/css/font-awesome.min.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.theme.default.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/animate.css",
              "src/assets/styles/main_styles.css",
              "src/assets/styles/responsive.css",
              "src/styles/overwrite.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/datatables.net-select-dt/css/select.dataTables.css",
              "node_modules/datatables.net-responsive-dt/css/responsive.dataTables.css",
              "node_modules/select2/dist/css/select2.min.css",
              "node_modules/ag-grid/dist/styles/ag-grid.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/bootstrap-social/bootstrap-social.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-select/dist/css/bootstrap-select.min.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "node_modules/material-icons/css/material-icons.min.css",
              "src/styles/grid.css",
              "src/styles/modal.css",
              "node_modules/mdbootstrap/css/mdb.css",
              "node_modules/ag-grid/dist/styles/ag-theme-balham.css",
              "src/assets/plugins/video-js/video-js.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.2.1.min.js",
              "src/assets/styles/bootstrap4/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/plugins/greensock/TweenMax.min.js",
              "src/assets/plugins/greensock/TimelineMax.min.js",
              "src/assets/plugins/scrollmagic/ScrollMagic.min.js",
              "src/assets/plugins/greensock/animation.gsap.min.js",
              "src/assets/plugins/greensock/ScrollToPlugin.min.js",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.js",
              "src/assets/plugins/easing/easing.js",
              "src/assets/plugins/parallax-js-master/parallax.min.js",
              "src/assets/js/custom.js",
              "node_modules/select2/dist/js/select2.full.min.js",
              "src/assets/plugins/video-js/video.min.js",
              "src/assets/plugins/video-js/Youtube.min.js",
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "testang2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "testang2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "testang2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "testang2-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "testang2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "testang2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "testang2"
}